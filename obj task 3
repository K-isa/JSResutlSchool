function handleObject(obj, key, action = 'get') {
  if (action === 'get') {
    return obj[key];
  } else if (action === 'delete') {
    delete obj[key];
    return obj;
  } else if (action === 'add') {
    obj.key = ''
    return obj;
  }
}

const student = {
  name: 'Maxim',
  programmingLanguage: 'JavaScript',
}

const result = handleObject(student, 'programmingLanguage', 'add');
console.log('result', result);
